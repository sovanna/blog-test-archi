  "scripts": {
    "test": "./node_modules/.bin/istanbul cover ./node_modules/.bin/_mocha -- --compilers js:babel-register --require babel-polyfill --recursive",
    "coveralls": "npm test && cat ./coverage/lcov.info | ./node_modules/.bin/coveralls",
    "lint": "./node_modules/.bin/eslint src/**/*.js",
    "prestart": "npm install && npm run lint",
    "start": "TMPDIR=/tmp NODE_DEBUG=nice NODE_ENV=development ./node_modules/.bin/babel-watch --extensions '[.js]' --exclude src/app src/router.js",
    "build": "rm -rf ./dist && ./node_modules/.bin/babel src -d dist --ignore node_modules,**/*.test.js && cp -r src/views dist/ && NODE_ENV=production ./node_modules/.bin/webpack -p --config ./webpack.prod.js",
    "serve": "NODE_ENV=production node dist/router.js"
  },
